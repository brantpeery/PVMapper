<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="/lpresources/style.css" rel="stylesheet" type="text/css" />
    <script language="javascript" src="/lpresources/js/jquery.min.js" type="text/javascript"></script>
    <script src="/lpresources/js/swfobject/swfobject.js" type="text/javascript"></script>

    @RenderSection("header", false)
</head>
<body>
    <!--Header Begin-->
    <div id="header">
        <div class="center">
            <div id="logo"><a href="#">PVMapper</a></div>
            <!--Menu Begin-->
            <div id="menu">
                <ul>
                    <li><a class="menuButton active" href="/"><span>Home</span></a></li>
                    <li><a class="menuButton" href="/app/" target="_blank"><span>Launch App</span></a></li>
                    <li><a class="menuButton" href="https://pvmapper.codeplex.com/documentation" target="_blank"><span>Technical Details</span></a></li>
                    <li><a class="menuButton" href="SiteDetail/feedback"><span>Contact Us</span></a></li>
                </ul>
            </div>
            <!--Menu END-->
        </div>
    </div>
    <!--Header END-->

    @RenderBody()

    <!--Footer Begin-->
    <div id="footer">
    </div>
    <!--Footer END-->

</body>
</html>
